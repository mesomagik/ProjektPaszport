<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.l03.projektpaszport.Fragment_OMnie_info_podstawowe">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="przyjmowanie_jedzenia"
                android:id="@+id/tvprzyjmowanie_jedzenia"
                android:layout_below="@+id/textView3"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Przyjmowanie jedzenia"
                android:id="@+id/textView3"
                android:layout_alignParentStart="true"
                android:textColor="#000"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Przyjmowanie płynów"
                android:id="@+id/przyjmowanie_plynów"
                android:layout_below="@+id/tvprzyjmowanie_jedzenia"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="przyjmowanie_plynów"
                android:id="@+id/tvprzyjmowanie_plynów"
                android:layout_below="@+id/przyjmowanie_plynów"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Moje Bezpieczeństwo"
                android:id="@+id/moje_bezpieczenstwo"
                android:layout_below="@+id/tvprzyjmowanie_plynów"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="moje_bezpieczenstwo"
                android:id="@+id/tvmoje_bezpieczenstwo"
                android:layout_below="@+id/moje_bezpieczenstwo"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Korzystanie z toalety"
                android:id="@+id/textView8"
                android:layout_below="@+id/tvmoje_bezpieczenstwo"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="korzystanie_z_toalety"
                android:id="@+id/tvkorzystanie_z_toalety"
                android:layout_below="@+id/textView8"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Opieka osobista"
                android:id="@+id/opieka_osobista"
                android:layout_below="@+id/tvkorzystanie_z_toalety"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="opieka_osobista"
                android:id="@+id/tvopieka_osobista"
                android:layout_below="@+id/opieka_osobista"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Sen"
                android:id="@+id/sen"
                android:layout_below="@+id/tvopieka_osobista"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="sen"
                android:id="@+id/tvsen"
                android:layout_below="@+id/sen"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="7dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Alergie"
                android:id="@+id/alergie"
                android:layout_below="@+id/tvsen"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="alergie"
                android:id="@+id/tvalergie"
                android:layout_below="@+id/alergie"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>
    </ScrollView>

</FrameLayout>
