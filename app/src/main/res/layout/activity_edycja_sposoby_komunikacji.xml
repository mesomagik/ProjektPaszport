<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_edycja_sposoby_komunikacji"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.l03.projektpaszport.EdycjaSposobyKomunikacjiActivity"
    android:layout_alignParentTop="false">

    <TextView
        android:text="Moje zmysÅ‚y"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:textColor="@android:color/black"
        android:layout_alignParentTop="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:layout_below="@+id/textView14"
        android:layout_alignParentStart="true"
        android:id="@+id/etMojeZmysly"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="false" />

    <TextView
        android:text="Przekazywanie emocji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etMojeZmysly"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView16"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:textColor="@android:color/black"
        android:layout_alignParentTop="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:layout_below="@+id/textView16"
        android:layout_centerHorizontal="true"
        android:id="@+id/etPrzekazywanieEmocji"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="false" />

    <TextView
        android:text="Charakterystyczne zachowania"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="@android:color/black"
        android:layout_below="@+id/etPrzekazywanieEmocji"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/etCharakterystyczneZachowania"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

    <Button
        android:text="Edytuj dane"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etCharakterystyczneZachowania"
        android:layout_alignParentEnd="true"
        android:id="@+id/bEdytujDane"
        style="@style/Widget.AppCompat.Button.Colored" />
</RelativeLayout>
